{
  "version": 2,
  "builds": [
    {
      "src": "recipes/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.12"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "recipes/wsgi.py"
    }
  ],
  "buildCommand": "./build_files.sh"
}
